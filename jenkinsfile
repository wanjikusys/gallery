pipeline{
    agent any
    tools{
        nodejs 'node23'
    }
    stages{
        stage ("fetching code from github") {
            steps{
             git branch:"master" , url:"https://github.com/wanjikusys/gallery.git"
            }
        }
        stage ("installing npm") {
            steps{
                sh 'npm install'
            }
        }
    }
}